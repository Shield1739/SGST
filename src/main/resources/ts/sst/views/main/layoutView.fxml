<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.ToggleButton?>
<?import javafx.scene.control.ToggleGroup?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="720.0" prefWidth="1280.0" stylesheets="@../../../../../../java/ts/sst/css/layoutView.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ts.sst.controllers.main.LayoutController">
   <children>
      <BorderPane prefHeight="720.0" prefWidth="1280.0">
         <left>
            <AnchorPane prefHeight="200.0" prefWidth="200.0" styleClass="box" BorderPane.alignment="CENTER">
               <children>
                  <Pane prefHeight="70.0" prefWidth="200.0">
                     <children>
                        <Text layoutX="59.0" layoutY="54.0" strokeType="OUTSIDE" strokeWidth="0.0" text="SST">
                           <font>
                              <Font name="System Bold" size="48.0" />
                           </font>
                        </Text>
                     </children>
                  </Pane>
                  <ScrollPane layoutY="70.0" prefHeight="620.0" prefWidth="200.0" styleClass="scroll">
                     <content>
                        <VBox prefHeight="620.0" prefWidth="200.0">
                           <children>
                              <VBox prefHeight="620.0" prefWidth="100.0" styleClass="box-menus">
                                 <children>
                                    <ToggleButton fx:id="toggleButtonPrestamos" alignment="CENTER_LEFT" mnemonicParsing="false" onAction="#handleToggleButtonPrestamos" prefWidth="200.0" selected="true" text="Prestamos">
                                       <toggleGroup>
                                          <ToggleGroup fx:id="groupMenus" />
                                       </toggleGroup>
                                       <styleClass>
                                          <String fx:value="menus" />
                                          <String fx:value="menu-group" />
                                       </styleClass>
                                    </ToggleButton>
                                    <VBox fx:id="toggleVboxPrestamos" prefWidth="200.0" styleClass="box-submenus">
                                       <children>
                                          <ToggleButton fx:id="subTogglePrestamosEquipos" alignment="CENTER_LEFT" mnemonicParsing="false" onAction="#handleSubTogglePrestamosEquipos" styleClass="submenus" text="Prestamos de Equipos">
                                             <toggleGroup>
                                                <ToggleGroup fx:id="groupPrestamos" />
                                             </toggleGroup>
                                          </ToggleButton>
                                          <ToggleButton fx:id="subTogglePrestamosLabs" alignment="CENTER_LEFT" mnemonicParsing="false" onAction="#handleSubTogglePrestamosLabs" styleClass="submenus" text="Prestamos de Laboratorios" toggleGroup="$groupPrestamos" />
                                       </children>
                                    </VBox>
                                    <ToggleButton fx:id="toggleButtonInventario" alignment="CENTER_LEFT" mnemonicParsing="false" onAction="#handleToggleButtonInventario" prefWidth="200.0" selected="true" text="Inventario" toggleGroup="$groupMenus">
                                       <styleClass>
                                          <String fx:value="menus" />
                                          <String fx:value="menu-group" />
                                       </styleClass>
                                    </ToggleButton>
                                 </children>
                              </VBox>
                           </children>
                        </VBox>
                     </content>
                  </ScrollPane>
                  <AnchorPane layoutY="690.0" prefHeight="60.0" prefWidth="200.0" styleClass="box-user">
                     <children>
                        <Rectangle arcHeight="5.0" arcWidth="5.0" fill="DODGERBLUE" height="50.0" layoutX="9.0" layoutY="5.0" stroke="BLACK" strokeType="INSIDE" width="50.0" />
                        <Label fx:id="userCorreoLabel" layoutX="60.0" layoutY="5.0" styleClass="lb-user" text="correoLabel" />
                        <Label fx:id="userTipoLabel" layoutX="60.0" layoutY="20.0" styleClass="lb-user" text="tipoLabel" />
                        <Button layoutX="141.0" layoutY="25.0" mnemonicParsing="false" styleClass="menu-exit" text="EXIT" />
                     </children>
                  </AnchorPane>
               </children>
            </AnchorPane>
         </left>
         <center>
            <AnchorPane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
         </center>
      </BorderPane>
   </children>
</AnchorPane>
